    "core-js": "^2.6.5",
    "coreapi": "^0.1.1",
    "uikit": "^3.0.2",
    "vue": "^2.6.10",
    "vue-chunk": "^1.0.0",
    "vue-flatpickr-component": "^8.1.2",
    "vue-json-edit": "^1.2.0",
